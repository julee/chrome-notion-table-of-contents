{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": false,
    "rootDir": "./src",
    "outDir": "./dist/js",
    "noEmitOnError": true,
    "jsx": "react",
    "resolveJsonModule": true
  },
  "include": ["./src"]
}
